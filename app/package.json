{
	"name": "life-recorder",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"doctor": "rm -rf ios/ && bunx expo-doctor --verbose",
		"prebuild:dev": "APP_VARIANT=development bunx expo prebuild --platform ios --clean",
		"prebuild": "bunx expo prebuild --platform ios --clean",
		"start": "APP_VARIANT=development expo start",
		"reset-project": "node ./scripts/reset-project.js",
		"android": "expo run:android",
		"ios": "expo run:ios --device",
		"web": "expo start --web",
		"test": "jest --watchAll",
		"lint": "biome check . --formatter-enabled=true --linter-enabled=true --organize-imports-enabled=true --diagnostic-level error --write",
		"typecheck": "tsc --project tsconfig.json --noEmit --pretty",
		"build:preview": "eas build --profile preview --platform ios"
	},
	"jest": {
		"preset": "jest-expo"
	},
